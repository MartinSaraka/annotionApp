{
  "name": "hiat-app",
  "productName": "AnnotAid",
  "description": "Histopathology image annotation tool",
  "author": "PeterSkriba <peter.skriba@gmail.com> (https://github.com/PeterSkriba)",
  "repository": "https://github.com/histopathology-image-annotation-tool/hiat-app.git",
  "homepage": "https://github.com/histopathology-image-annotation-tool/hiat-app#readme",
  "main": "./out/main/index.js",
  "license": "MIT",
  "version": "0.2.0",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn typecheck:node && yarn typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "yarn typecheck && electron-vite build",
    "build:win": "PLATFORM=win32 ARCH=x64 yarn build && electron-builder --win --x64 --config",
    "build:mac-arm": "PLATFORM=darwin ARCH=arm64 electron-vite build && electron-builder --mac --config",
    "build:mac-intel": "PLATFORM=darwin ARCH=x64 electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "patch:stitches": "npx patch-package @stitches/react",
    "patch:annotorious": "npx patch-package @recogito/annotorious-openseadragon",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.8.0",
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^1.0.2",
    "@minoru/react-dnd-treeview": "^3.4.4",
    "@openseadragon-imaging/openseadragon-imaginghelper": "^2.2.2",
    "@radix-ui/colors": "^2.0.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-context-menu": "^2.1.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toolbar": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@recogito/annotorious-openseadragon": "^2.7.14",
    "@recogito/annotorious-selector-pack": "^0.5.3",
    "@recogito/annotorious-shape-labels": "^0.2.3",
    "@stitches/react": "^1.2.8",
    "apollo-link-rest": "^0.9.0",
    "classnames": "^2.3.2",
    "electron-updater": "^5.3.0",
    "electron-util": "^0.17.2",
    "formik": "^2.4.4",
    "framer-motion": "^10.16.1",
    "geojson": "^0.5.0",
    "get-port": "^7.0.0",
    "graphql": "^16.7.1",
    "i18next": "^23.5.1",
    "ky": "^1.1.3",
    "lodash": "^4.17.21",
    "luxon": "^3.4.4",
    "openseadragon": "^4.1.0",
    "path-browserify": "^1.0.1",
    "qs": "^6.11.2",
    "react-accessible-treeview": "^2.8.0",
    "react-dnd": "^16.0.1",
    "react-grid-layout": "^1.3.4",
    "react-i18next": "^13.2.2",
    "react-inlinesvg": "^4.1.0",
    "react-router-dom": "^6.14.2",
    "react-sizeme": "^3.0.2",
    "remove": "^0.1.5",
    "smartlook-client": "^8.2.0",
    "uuid": "^9.0.1",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@types/geojson": "^7946.0.13",
    "@types/i18next": "^13.0.0",
    "@types/lodash": "^4.14.196",
    "@types/luxon": "^3.3.7",
    "@types/node": "^18.16.16",
    "@types/openseadragon": "^3.0.6",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/react-grid-layout": "^1.3.2",
    "@types/react-i18next": "^8.1.0",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^24.4.1",
    "electron-builder": "^24.6.4",
    "electron-vite": "^1.0.23",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "patch-package": "^8.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "engines": {
    "node": "^18.x",
    "yarn": "^1.x",
    "npm": "please-use-yarn"
  }
}
